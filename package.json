    "migration:generate": "yarn ts-node ./node_modules/typeorm/cli.js migration:generate -n PostMigration -f ormconfig.js"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "env-var": "^7.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32",
    "typeorm-naming-strategies": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/uuid": "^8.3.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
