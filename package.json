    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.32",
    "typeorm-naming-strategies": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
